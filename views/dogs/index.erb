<h1>Estos son mis perros:</h1>

<ul>
  <% @dogs.each do |dog| %>
      <li><a href="/dogs/<%= dog.id %>"> <%= dog.name %> </a></li>
      <li>
        <form action="/dogs/<%= dog.id %>" method="post">
          <input type="hidden" name="_method" value="delete">
          <input type="submit" value="Eliminar perro">
        </form>
      </li>
  <% end %>
</ul>
